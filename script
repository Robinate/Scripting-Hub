Info_2.Name = "Info"
Info_2.Parent = Bleach
Info_2.BackgroundColor3 = Color3.fromRGB(164, 136, 227)
Info_2.BorderSizePixel = 0
Info_2.Position = UDim2.new(0.997997284, 0, -0.00929556787, 0)
Info_2.Size = UDim2.new(0, 50, 0, 45)
Info_2.ZIndex = 2
Info_2.Font = Enum.Font.GothamMedium
Info_2.Text = ""
Info_2.TextColor3 = Color3.fromRGB(255, 255, 255)
Info_2.TextScaled = true
Info_2.TextSize = 14.000
Info_2.TextWrapped = true

UICorner_18.CornerRadius = UDim.new(0, 4)
UICorner_18.Parent = Info_2

Shadow_6.Name = "Shadow"
Shadow_6.Parent = Info_2
Shadow_6.BackgroundColor3 = Color3.fromRGB(90, 74, 124)
Shadow_6.BorderSizePixel = 0
Shadow_6.Size = UDim2.new(1, 0, 1, 4)

UICorner_19.CornerRadius = UDim.new(0, 4)
UICorner_19.Parent = Shadow_6

ImageButton_2.Parent = Info_2
ImageButton_2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ImageButton_2.BackgroundTransparency = 1.000
ImageButton_2.Position = UDim2.new(0, 0, -0.0114444308, 0)
ImageButton_2.Size = UDim2.new(0, 50, 0, 49)
ImageButton_2.ZIndex = 2
ImageButton_2.Image = "rbxassetid://3926305904"
ImageButton_2.ImageRectOffset = Vector2.new(524, 444)
ImageButton_2.ImageRectSize = Vector2.new(36, 36)

